&pmic_fgu {
	monitored-battery = <&bat &bat1 &bat2 &bat3>;
	sprd,calib-resistance-micro-ohms = <10000>;
	sprd,comp-resistance-mohm = <13>;
	io-channels = <&pmic_adc 0>, <&pmic_adc 3>,<&pmic_adc 14>;
	io-channel-names = "bat-temp", "adc-batt-id", "charge-vol";
};

&pmic_pd {
	status = "okay";
	sprd,syscon-aon-apb = <&aon_apb_regs>;
	usb_con: connector {
		compatible = "usb-c-connector";
		label = "USB-C";
		power-role = "dual";
		try-power-role = "sink";
		source-pdos = <PDO_FIXED(5000, 400, PDO_FIXED_USB_COMM)>;
		sink-pdos = <PDO_FIXED(5000, 3000, PDO_FIXED_USB_COMM)
				PDO_PPS_APDO(3300, 11000, 3000)>;
		op-sink-microwatt = <10000000>;
	};
};

&pmic_fchg {
	sprd,input-voltage-microvolt = <5000000>;
	sprd,support-sfcp;
 	phys = <&hsphy>;
	status = "okay";
};
/ {
	charger-manager {
		cm-fullbatt-voltage = <4350000>;
		cm-fullbatt-vchkdrop-volt = <50000>;
		cm-fullbatt-current = <240000>;
		cm-fullbatt-comp-voltage = <32000>;
		cm-thermal-limit = <2>;
		cm-fullbatt-advance-level = <960>;
		/* in deci centigrade */
		cm-battery-cold = <0>;
		cm-battery-hot = <550>;
		cm-battery-temp-diff = <30>;
		cm-tickle-time-out = <180>;

		cm-disable-charger-type-jeita;
		/* sprd_fchg parameter */
		sprd,support-fchg;
		sprd,pd-fixed-max-microwatt = <18000000>;

		extcon = <&pmic_typec>, <&pmic_pd>;

		cm-jeita-temp-table = <1000 1030 0 4400000>,
				<1100 1130 1400000 4400000>,
				<1150 1180 4800000 4400000>,
				<1450 1420 4800000 4400000>,
				<1550 1520 2400000 4100000>;

		cm-chargers = "bq2560x_charger";
		cm-hardware-psy = "bq2560x_charger";
		cm-hardware2-psy = "bq2560x_slave_charger";
		zte-cm-hardware-psy = "zte_bq2560x_charger";
		zte-cm-hardware2-psy = "zte_bq2560x_slave_charger";
		monitored-battery = <&bat &bat1 &bat2 &bat3>;
		phys = <&hsphy>;
	};

	zte_misc: zte_misc {
		status = "okay";
		compatible = "zte-misc";
	};
};

&i2c5 {
	status = "okay";
	bq2560x_chg: charger@6b {
		compatible = "ti,bq2560x_chg";
		reg = <0x6b>;
		phys = <&hsphy>;
		monitored-battery = <&bat &bat1>;
		extcon = <&extcon_gpio>;
		vddvbus:otg-vbus {
			regulator-name = "vddvbus";
		};
	};
};

&i2c4 {
	status = "okay";
	bq2560x_chg2: charger2@6b {
		compatible = "ti,bq2560x_chg2";
		reg = <0x6b>;
		phys = <&hsphy>;
		monitored-battery = <&bat &bat1 &bat2 &bat3>;
		extcon = <&extcon_gpio>;
		enable-gpios = <&ap_gpio 54 0>;
		role-slave;
	};
};

